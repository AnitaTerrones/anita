@{
    List<Persona> personas = ((List<Persona>)Model); // forma B
}

<h1>Personas</h1>

<a href="/persona/create">Crear Cuenta</a>

<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Apellidos</th>
            <th>FNacimiento</th>
            <th>DNI</th>
            <th>Genero</th>
            <th>Ciudad</th>
            <th>Direccion</th>
            <th>Correo</th>
            <th>Username</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < personas.Count; i++)
        {
            Persona item = personas[i];
        <tr>
            <td></td>
            <td>@item.Nombre</td>
            <td>@item.Apellidos</td>
            <td>@item.FNacimiento</td>
            <td>@item.DNI S/.</td>
            <td>@item.Genero</td>
            <td>@item.Ciudad</td>
            <td>@item.Direccion</td>
            <td>@item.Correo</td>
            <td>@item.Username</td>
            <td>@item.Password</td>
        </tr>
        }
    </tbody>
</table>